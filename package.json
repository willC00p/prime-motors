    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@types/react": "^19.1.10",
    "@types/recharts": "^2.0.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "recharts": "^3.1.2",
    "xlsx": "^0.18.5"
  }
}
